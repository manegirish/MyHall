<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.technoindians.myhall">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.technoindians.main.LoginActivity"/>
        <activity android:name="com.technoindians.main.MainActivity"/>
        <activity android:name="com.technoindians.calendar.EventListActivity"/>
        <activity android:name="com.technoindians.gallery.FullImageActivity"/>
    </application>

</manifest>